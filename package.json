{
  "name": "mec-registry-app",
  "version": "1.0.0",
  "description": "Learning Basic State Idea of Ethereum",
  "main": "index.js",
  "scripts": {
    "lint": "next lint",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "anvil": "./scripts/setenv.sh .env && anvil",
    "wagmi": ". ./scripts/setenv.sh .env && wagmi generate && cp contract/out/IssuerRegistry.sol/IssuerRegistry.json src/abis/",
    "deploy": ". ./scripts/setenv.sh .env && forge create contract/src/IssuerRegistry.sol:IssuerRegistry --rpc-url $FORGE_RPC_URL --private-key $FORGE_PRIVATE_KEY"
  },
  "keywords": [
    "Vite",
    "express",
    "Javascript"
